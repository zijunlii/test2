<div class="container">
  <div class="row">
    <div class="col-md-12">
        <h2>Student List</h2>
        <hr />
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Student Id</th>
                        <th>Name</th>
                        <th>Program</th>
                        <th>GPA</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each students}}
                    <tr>
                        <td>{{this.studId}}</td>
                        <td>{{this.name}}</td>
                        <td>{{this.program}}</td>
                        <td>{{this.gpa}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>